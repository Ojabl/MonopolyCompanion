<Page x:Class="MonopolyCompanion.Pages.NewGame"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MonopolyCompanion.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="1000"
      Title="NewGame">

    <Grid Background="White">
        <Button x:Name="BackBtn" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="100" Height="50" Margin="10" Command="NavigationCommands.BrowseBack"/>
        <Label Content="New Game" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    </Grid>
</Page>
